# DBC4AT files
WMUI_INSTALL_IMAGE_FILE=/tmp/dbc-products.zip
WMUI_PATCH_FIXES_IMAGE_FILE=/tmp/dbc-fixes.zip

# For Database Configurator
WMUI_DBSERVER_HOSTNAME=postgresql-db-server
