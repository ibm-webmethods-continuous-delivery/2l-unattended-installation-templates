H_WMUI_HOME=

# Should keep this on latest and smallest compatible image
WMUI_TEST_POSTGRESQL_IMAGE=postgres:16-alpine3.22
WMUI_TEST_PORT_PREFIX=458

# Everything below may be left as is for the test purposes

# Internal variables required by the template (change is optional)
WMUI_INSTALL_INSTALLER_BIN=/tmp/installer.bin
WMUI_INSTALL_INSTALLER_BIN=/tmp/installer.bin
WMUI_PATCH_SUM_BOOTSTRAP_BIN=/tmp/sum-bootstrap.bin
